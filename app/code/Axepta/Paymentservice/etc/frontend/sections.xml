<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Customer:etc/sections.xsd">
    <action name="axepta/payment/notify">
        <section name="cart"/>
        <section name="checkout-data"/>
    </action>
    <action name="axepta/payment/error">
        <section name="cart"/>
        <section name="checkout-data"/>
    </action>
    <action name="axepta/payment/success">
        <section name="cart"/>
        <section name="checkout-data"/>
    </action>
    <action name="axepta/payment/callback">
        <section name="cart"/>
        <section name="checkout-data"/>
    </action>
</config>
